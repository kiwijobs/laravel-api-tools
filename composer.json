{
    "name": "kiwijobs/laravel-api-tools",
    "description": "Some tools for Laravel API development, nothing really new, just wrappers and helper stuff",
    "license": "MIT",    
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "spatie/laravel-fractal": "^5.0",
        "dingo/api": "^2.4|3.0.0-rc3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8",
        "orchestra/database": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Joselfonseca\\LaravelApiTools\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Joselfonseca\\LaravelApiTools\\Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joselfonseca\\LaravelApiTools\\Providers\\LaravelApiToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
